<h3>All Works</h3>

<% @works.each do |w| %>
  <p>
    <%#= image_tag( w.image, :class => "thumbnail" ) %>
    <%#= link_to(image_tag(w.image, :class => "thumbnail"), "/works/#{w.id}") %>
    <%#= link_to( image_tag(w.image, :class => "thumbnail"), work_path(w.id) ) %>
    <%= link_to( image_tag(w.image, :class => "thumbnail"), work_path(w) ) %>
    <%#= link_to( image_tag(w.image, :class => "thumbnail"), w ) %>
  </p>
<% end %>
